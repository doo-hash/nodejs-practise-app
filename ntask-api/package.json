{
  "name": "ntask-api",
  "version": "1.0.0",
  "description": "ntask api my version - referred from book",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "dev": "nodemon index.js",
    "start": "npm run apidoc && node index.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc",
    "coverage": "nyc --reporter=html --reporter=text mocha --exit --timeout 30000"
  },
  "apidoc": {
    "name": "Node Task API - Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "keywords": [
    "nodejs",
    "api",
    "testing",
    "sequelize",
    "mocha",
    "chai",
    "supertest",
    "passport",
    "passport-jwt"
  ],
  "author": "M.Soujanya",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jwt-simple": "^0.5.6",
    "mysql2": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "apidoc": "^0.54.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  }
}

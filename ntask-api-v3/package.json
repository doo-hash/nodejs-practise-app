{
  "name": "ntask-api-v3",
  "version": "1.0.0",
  "description": "ntaskapi with express, sql, sequelize, passport-local and testing with mocha, chai, supertest",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "test": "mocha --timeout 30000 --recursive --exit",
    "start": "npm run apidoc && node index.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc",
    "coverage": "nyc --reporter=html --reporter=text mocha --exit --timeout 30000"
  },
  "keywords": [
    "express",
    "api",
    "sequelize",
    "mysql",
    "passport-local",
    "mocha",
    "chai",
    "supertest"
  ],
  "author": "M.Soujanya",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mysql2": "^3.1.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.28.2"
  },
  "devDependencies": {
    "apidoc": "^0.54.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  }
}
